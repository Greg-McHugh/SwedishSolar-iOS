    pod 'AWSCognitoAuth'
    pod 'AWSLambda'
    pod 'SwiftyJSON'
    pod 'SVProgressHUD'
    pod 'Moscapsule', :git => 'https://github.com/flightonary/Moscapsule.git'
    pod 'OpenSSL-Universal'
    
end

